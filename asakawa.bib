@article{2014Imai,
title={The sound symbolism bootstrapping hypothesis for language acquisition and language evolution},
author={Mutsumi Imai and Sotaro Kita},
journal={Philosophical transactions of the royal society, {B}: Biological sciences},
volume={369},
number={1651},
year={2014},
pages={20130298},
abstract={Sound symbolism is a non-arbitrary relationship between speech sounds and meaning. We review evidence that, contrary to the traditional view in linguistics, sound symbolism is an important design feature of language, which affects online processing of language, and most importantly, language acquisition.
We propose the sound symbolism bootstrapping hypothesis, claiming that (i) pre-verbal infants are sensitive to sound symbolism, due to a biologically endowed ability to map and integrate multi-modal input, (ii) sound symbolism helps infants gain referential insight for speech sounds, (iii) sound symbolism helps infants and toddlers associate speech sounds with their referents to establish a lexical representation and (iv) sound symbolism helps toddlers learn words by allowing them to focus on referents embedded in a complex scene, alleviating Quine’s problem. We further explore the possibility that sound symbolism is deeply related to language evolution, drawing the parallel between historical development of language across generations and ontogenetic development within individuals. Finally, we suggest that sound symbolism bootstrapping is a part of a more general phenomenon of bootstrapping by means of iconic representations, drawing on similarities and close behavioural links between sound symbolism and speech-accompanying iconic gesture.}
}

@book{2000Imai,
title={心の生得性-言語・概念獲得に生得的制約は必要か},
author={今井 むつみ},
yomi={Mutsumi Imai},
editor={日本認知科学会},
year={2000},
publisher={共立出版},
address={東京},
@Comment={「言語・概念獲得に生得的制約は必要か」というサブタイトルの内容の方がこの本のタイトルとしてはふさわしい．雑誌「認知科学」の特集号を元にして，６，７，８章にコネクショニストからのアプローチを追加してできた本である．恐らく編者にはまだ不満であろうと思われるコネクショニストモデルの論文も取り上げている所は評価できると思う．エルマンの本（邦訳のタイトルは認知発達と生得性）に刺激されて作ったとはじめにに書かれているとおり，言語獲得の生得性，どこまでがあらかじめ組み込まれていて，何が環境との相互作用によって創発するのかを議論している論文が掲載されている．専門書なので門外漢には全くわからないだろうし，専門にしているものにとっては「認知科学」ですでに読んでいるので買う必要は無い．よってこの本の想定する読者層がどのあたりなのか不明である．この分野を面白いと思う人間にとってはとても有益な本であるが，少しでも外れるとまるで理解不能であろう．少なくとも私には面白かった．（アマゾンのブックレビューに書いた内容）}
}

@book{2003Imai,
title={人が学ぶということ-認知学習論からの視点},
